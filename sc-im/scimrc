# Color Scheme

color "type=INPUT  fg=GREEN bg=DEFAULT_COLOR"
color "type=NORMAL fg=GREEN bg=DEFAULT_COLOR"
color "type=MODE   fg=GREEN bg=DEFAULT_COLOR"

color "type=WELCOME fg=CYAN bg=DEFAULT_COLOR bold=1"

color "type=CELL_SELECTION_SC fg=WHITE bg=BLACK"
color "type=CELL_ERROR        fg=RED   bg=DEFAULT_COLOR   bold=1"
color "type=CELL_NEGATIVE     fg=RED   bg=DEFAULT_COLOR"

color "type=NUMB       fg=CYAN      bg=DEFAULT_COLOR"
color "type=STRG       fg=WHITE   bg=DEFAULT_COLOR   bold=0"
color "type=DATEF      fg=YELLOW    bg=DEFAULT_COLOR"
color "type=EXPRESSION fg=GREEN       bg=DEFAULT_COLOR"

color "type=INFO_MSG   fg=CYAN      bg=DEFAULT_COLOR   bold=1"
color "type=ERROR_MSG  fg=RED       bg=DEFAULT_COLOR   bold=1"

color "type=HEADINGS     fg=CYAN bg=DEFAULT_COLOR bold=0" 
color "type=HEADINGS_ODD fg=CYAN bg=DEFAULT_COLOR bold=0"

# Formatting

set autocalc
set numeric
set numeric_decimal=0
set overlap
set xlsx_readformulas
